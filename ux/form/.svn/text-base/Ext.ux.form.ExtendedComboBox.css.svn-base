.ux-extcombo-icon {
	width:16px;
	height:16px;
	float:left;
	background-position: -1px -1px ! important;
	background-repeat:no-repeat ! important;
}
.ux-extcombo-icon-checked {
	/* correzione GDF per CNC
	background: transparent url(../js/ext/resources/images/default/menu/checked.gif);
	*/
	background: transparent url(../../ext/resources/images/default/menu/checked.gif);
}
.ux-extcombo-icon-unchecked {
	/* correzione GDF per CNC
	background: transparent url(../js/ext/resources/images/default/menu/unchecked.gif);
	*/
	background: transparent url(../../ext/resources/images/default/menu/unchecked.gif);
}


.ux-extcombo-fav-icon {
	width:16px;
	height:16px;
	float:right;
	background-position: -1px -1px ! important;
	background-repeat:no-repeat ! important;
}
.ux-extcombo-fav-icon-checked {
	background: transparent url(../images/icon1/star.png);

}
.ux-extcombo-fav-icon-unchecked {
	background: transparent url(../images/icon1/star_empty.png);
}

